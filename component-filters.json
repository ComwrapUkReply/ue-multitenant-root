  {
    "id": "standard-richtext",
    "rte": {
      "toolbar": {
        "format": ["bold", "italic", "underline"],
        "blocks": ["paragraph", "h2", "h3", "h4"],
        "alignment": ["left", "center", "right"],
        "indentation": ["indent", "outdent"],
        "list": ["bullet_list", "ordered_list"],
        "insert": ["link", "unlink"],
        "editor": ["removeformat"],
        "sections": [
          "format",
          "blocks",
          "alignment",
          "indentation",
          "list",
          "insert",
          "editor"
        ]
      },
      "actions": {
        "bold": {
          "tag": "strong",
          "shortcut": "Mod-B",
          "label": "Bold"
        },
        "italic": {
          "tag": "em",
          "shortcut": "Mod-I",
          "label": "Italic"
        },
        "underline": {
          "tag": "u",
          "shortcut": "Mod-U",
          "label": "Underline"
        },
        "bullet_list": {
          "wrapInParagraphs": true,
          "label": "Bullet List"
        },
        "ordered_list": {
          "wrapInParagraphs": true,
          "label": "Numbered List"
        },
        "link": {
          "hideTarget": false,
          "shortcut": "Mod-K",
          "label": "Add Link"
        },
        "h2": {
          "shortcut": "Mod-Alt-2",
          "label": "Heading 2"
        },
        "h3": {
          "shortcut": "Mod-Alt-3",
          "label": "Heading 3"
        },
        "h4": {
          "shortcut": "Mod-Alt-4",
          "label": "Heading 4"
        }
      }
    },
    "components": ["richtext", "text"]
  },
